{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/dist/index.html"
    },
    {
      "src": "/edit-recipe",
      "dest": "/dist/edit_recipe_page.html"
    },
    {
      "src": "/home",
      "dest": "/dist/home_page.html"
    },
    {
      "src": "/my-recipes",
      "dest": "/dist/my_recipe_page.html"
    },
    {
      "src": "/recipe",
      "dest": "/dist/recipe_page.html"
    },
    {
      "src": "/signup",
      "dest": "/dist/signup_page.html"
    }
  ],
  "outputDirectory": "dist",
  "buildCommand": "npm run build"
}
